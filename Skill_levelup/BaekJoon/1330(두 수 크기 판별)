using System;

namespace Algorithm
{
    class Programing
    {
        static void Main()
        {
            string nums = Console.ReadLine();
            string[] num = nums.Split();
            int a = int.Parse(num[0]);
            int b = int.Parse(num[1]);

            if (a > b)
                Console.WriteLine(">");
            else if (a < b)
                Console.WriteLine("<");
            else
                Console.WriteLine("==");
        }
    }
}
